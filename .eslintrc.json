{
    "extends": [
        "airbnb",
        "airbnb-typescript",
        "prettier"
    ],
    "parserOptions": {
        "project": [
            "./tsconfig.json"
        ]
    },
    "ignorePatterns": [
        "/dist",
        "/node_modules",
        "/parcel-cache"
    ],
    "rules": {
        "block-closing-brace-space-after": 0
    }
}