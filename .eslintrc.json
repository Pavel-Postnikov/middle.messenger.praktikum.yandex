{
    "extends": [
        "airbnb",
        "airbnb-typescript",
        "prettier"
    ],
    "parserOptions": {
        "project": [
            "./tsconfig.json"
        ]
    },
    "ignorePatterns": [
        "./dist",
        "./node_modules",
        "./parcel-cache"
    ],
    "rules": {
        "no-console": "off",
        "no-alert": "off",
        "class-methods-use-this": "off",
        "no-underscore-dangle": "off",
        "no-useless-escape": "off"
    }
}